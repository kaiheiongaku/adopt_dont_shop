<%= form_tag '/applications' do %>
  <h2>Start a New Application</h2>
  <%= label_tag :name %>
  <%= text_field_tag :name%>
  <%= label_tag :street %>
  <%= text_field_tag :street %>
  <%= label_tag :city %>
  <%= text_field_tag :city %>
  <%= label_tag :state %>
  <%= text_field_tag :state %>
  <%= label_tag :zip %>
  <%= text_field_tag :zip %>
  <%= label_tag :description %>
  <%= text_field_tag :description %>
  <%= submit_tag 'Submit' %>
<% end %>
